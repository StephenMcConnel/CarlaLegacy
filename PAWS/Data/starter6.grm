rule {P' option 6a - P_prefix, IP complement}
P' = IP
		<P' head> = <IP head>
		<IP head type prefix P> = +
	<P' head type stranded> = -
	<P' head type sentential> = +

rule {P' option 6b - P_suffix, IP complement}
P' = IP
		<P' head> = <DP head>
		<DP head type suffix P> = +
	<P' head type stranded> = -
	<P' head type sentential> = +

|*********************************************************
|prefix/suffix <= constraints done here down
|change rule options use d versions for Deg only,
|m versions for manner AdvP also
|none:0  initial:0,1  final:0,2  either:0,1,2  both:3+
rule {AdjP option 0 - no modifiers}
AdjP = Adj
	<AdjP head> = <Adj head>

rule {AdjP option 1d - degree modifiers initial }
AdjP = Deg Adj
	<AdjP head> = <Adj head>
	<Deg head type modifies_Adj> = +
	<AdjP head type prefix> <= <Deg head type prefix>

rule {AdjP option 1m - degree or AdvP modifiers initial }
AdjP = {Deg / AdvP} Adj
	<AdjP head> = <Adj head>
	<Deg head type modifies_Adj> = +
	<AdvP head type manner>      = +
	<AdjP head type prefix> <= <Deg head type prefix>
	<AdjP head type prefix> <= <AdvP head type prefix>

rule {AdjP option 2d - degree modifiers final }
AdjP = Adj Deg
	<AdjP head> = <Adj head>
	<Deg head type modifies_Adj> = +
	<AdjP head type suffix> <= <Deg head type suffix>

rule {AdjP option 2m - degree or AdvP modifiers final }
AdjP = Adj {Deg / AdvP}
	<AdjP head> = <Adj head>
	<Deg head type modifies_Adj> = +
	<AdvP head type manner>      = +
	<AdjP head type suffix> <= <Deg head type suffix>
	<AdjP head type suffix> <= <AdvP head type suffix>

rule {AdjP option 3d - degree modifiers both sides }
AdjP = Deg_1 Adj Deg_2
	<AdjP head> = <Adj head>
	<Deg_1 head type modifies_Adj> = +
	<Deg_2 head type modifies_Adj> = +
	<Deg_1 head type AdjP-initial> = +
	<Deg_2 head type AdjP-final>   = +
	<AdjP head type prefix> <= <Deg_1 head type prefix>
	<AdjP head type suffix> <= <Deg_2 head type suffix>

rule {AdjP option 3m - degree or AdvP modifiers both sides }
AdjP = Deg_1 / AdvP_1 Adj Deg_2 / AdvP_2
	<AdjP head> = <Adj head>
	<AdvP_1 head type manner>      = +
	<AdvP_2 head type manner>      = +
	<Deg_1 head type modifies_Adj> = +
	<Deg_2 head type modifies_Adj> = +
	<Deg_1 head type AdjP-initial> = +
	<Deg_2 head type AdjP-final>   = +
	<AdjP head type prefix> <= <Deg_1 head type prefix>
	<AdjP head type suffix> <= <Deg_2 head type suffix>
	<AdjP head type prefix> <= <AdvP_1 head type prefix>
	<AdjP head type suffix> <= <AdvP_2 head type suffix>

|***********************************************************
|change rule options  none:0  initial:0,1  final:0,2  either:0,1,2  both:3+
rule {AdvP option 0 - no modifiers}
AdvP = Adv
	<AdvP head> = <Adv head>

rule {AdvP option 1 - modifiers initial}
AdvP = Deg Adv
	<AdvP head> = <Adv head>
	<Deg head type modifies_Adv> = <Adv head type>
	<AdvP head type prefix> <= <Deg head type prefix>

rule {AdvP option 2 - modifiers final}
AdvP = Adv Deg
	<AdvP head> = <Adv head>
	<Deg head type modifies_Adv> = <Adv head type>
	<AdvP head type suffix> <= <Deg head type suffix>

rule {AdvP option 3 - modifiers both sides}
AdvP = Deg_1 Adv Deg_2
	<AdvP head> = <Adv head>
	<Deg_1) head type modifies_Adv> = <Adv head type>
	<Deg_2) head type modifies_Adv> = <Adv head type>
	<Deg_1 head type AdvP-initial> = +
	<Deg_2 head type AdvP-final>   = +
	<AdvP head type prefix> <= <Deg_1 head type prefix>
	<AdvP head type suffix> <= <Deg_2 head type suffix>


|***********************************************************
|change rule options  none:0  initial:0,1  final:0,2  either:0,1,2  both:3+
rule {QP option 0 - no modifiers}
QP = Q
	<QP head> = <Q head>

rule {QP option 1 - modifiers initial}
QP = Deg Q
	<QP head> = <Q head>
	<Deg head type modifies_Q> = +
	<QP head type prefix> <= <Deg head type prefix>

rule {QP option 2 - modifiers final}
QP = Q Deg
	<QP head> = <Q head>
	<Deg head type modifies_Q> = +
	<QP head type suffix> <= <Deg head type suffix>

rule {QP option 3 - modifiers both sides}
QP = Deg_1 Q Deg_2
	<QP head> = <Q head>
	<Deg_1 head type modifies_Q> = +
	<Deg_2 head type modifies_Q> = +
	<Deg_1 head type QP-initial> = +
	<Deg_2 head type QP-final>   = +
	<QP head type prefix> <= <Deg_1 head type prefix>
	<QP head type suffix> <= <Deg_2 head type suffix>

|**********************************************************
|use i versions for head initial, f versions for head final
|modifiers, none:0  initial:0,1  final:0,2  either:0,1,2  both:0,3(1&2asneeded)
rule {QP as Numbers option 0i - no modifiers, head initial}
QP = Num (Conj) (Num_1 (Conj_1) (Num_2 (Num_3 (Num_4 (Num_5)))))
	<QP head> = <Num head>
		<Conj head type conjoins_DP> = +
		<Conj_1 head type conjoins_DP> = +

rule {QP as Numbers option 0f - no modifiers, head final}
QP = (((((Num_5) Num_4) Num_3) Num_2) (Conj_1) Num_1) (Conj) Num
	<QP head> = <Num head>
		<Conj head type conjoins_DP> = +
		<Conj_1 head type conjoins_DP> = +

rule {QP as Numbers option 1i - modifiers initial, head initial}
QP = Adj Num (Conj) (Num_1 (Conj_1) (Num_2 (Num_3 (Num_4 (Num_5)))))
	<QP head> = <Num head>
	<Adj head type ordinal> = +
		<Conj head type conjoins_DP> = +
		<Conj_1 head type conjoins_DP> = +

rule {QP as Numbers option 1f - modifiers initial, head final}
QP = Adj (((((Num_5) Num_4) Num_3) Num_2) (Conj_1) Num_1) (Conj) Num
	<QP head> = <Num head>
	<Adj head type ordinal> = +
		<Conj head type conjoins_DP> = +
		<Conj_1 head type conjoins_DP> = +

rule {QP as Numbers option 2i - modifiers final, head initial}
QP = Num (Conj) (Num_1 (Conj_1) (Num_2 (Num_3 (Num_4 (Num_5))))) Adj
	<QP head> = <Num head>
	<Adj head type ordinal> = +
		<Conj head type conjoins_DP> = +
		<Conj_1 head type conjoins_DP> = +

rule {QP as Numbers option 2f - modifiers final, head final}
QP = (((((Num_5) Num_4) Num_3) Num_2) (Conj_1) Num_1) (Conj) Num Adj
	<QP head> = <Num head>
	<Adj head type ordinal> = +
		<Conj head type conjoins_DP> = +
		<Conj_1 head type conjoins_DP> = +

rule {QP as Numbers option 3i - modifiers both sides, head initial}
QP = Adj Num (Conj) (Num_1 (Conj_1) (Num_2 (Num_3 (Num_4 (Num_5))))) Adj_1
	<QP head> = <Num head>
	<Adj head type ordinal> = +
	<Adj_1 head type ordinal> = +
	<Adj head type QP-initial> = +
	<Adj_1 head type QP-final>   = +
		<Conj head type conjoins_DP> = +
		<Conj_1 head type conjoins_DP> = +

rule {QP as Numbers option 3f - modifiers both sides, head final}
QP = Adj (((((Num_5) Num_4) Num_3) Num_2) (Conj_1) Num_1) (Conj) Num Adj_1
	<QP head> = <Num head>
	<Adj head type ordinal> = +
	<Adj_1 head type ordinal> = +
	<Adj head type QP-initial> = +
	<Adj_1 head type QP-final>   = +
		<Conj head type conjoins_DP> = +
		<Conj_1 head type conjoins_DP> = +
