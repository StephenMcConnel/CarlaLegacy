<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE page SYSTEM "..\Data\PAWSSKHtmlMapper.dtd">
<?xml-stylesheet type="text/xsl" href="..\Templates\PAWSSKHtmlMapper.xsl"?>
<page id="PronAgr">
	<title level="2">Pronouns and Proper Nouns - Pronoun Agreement</title>
<instruction>Add the relevant agreement features to each pronoun in the lexicon, as shown in the chart.  If a pronoun is not specified for a particular type of agreement feature, do not add any feature of that type.<br/><br/>
<featureChart>
	<featureRow>
		<featureType>person</featureType>
		<featureItem>first</featureItem>
		<featureExplanation>first person, with no exclusive-inclusive distinction</featureExplanation>
</featureRow>
<featureRow>
	<featureType>&nbsp;</featureType>
	<featureItem>first_exclusive</featureItem>
	<featureExplanation>first person, cannot include the hearer</featureExplanation>
</featureRow>
	<featureRow>
		<featureType>&nbsp;</featureType>
		<featureItem>first_inclusive</featureItem>
		<featureExplanation>first person, and including the hearer</featureExplanation>
	</featureRow>
	<featureRow>
		<featureType>&nbsp;</featureType>
		<featureItem>second</featureItem>
		<featureExplanation>second person</featureExplanation>
	</featureRow>
	<featureRow>
		<featureType>&nbsp;</featureType>
		<featureItem>third</featureItem>
		<featureExplanation>third person</featureExplanation>
	</featureRow>
	<featureRow>
		<featureType>number</featureType>
		<featureItem>singular</featureItem>
		<featureExplanation>singular in number</featureExplanation>
	</featureRow>
	<featureRow>
		<featureType>&nbsp;</featureType>
		<featureItem>dual</featureItem>
		<featureExplanation>two in number</featureExplanation>
	</featureRow>
	<featureRow>
		<featureType>&nbsp;</featureType>
		<featureItem>plural</featureItem>
		<featureExplanation>plural in number</featureExplanation>
	</featureRow>
	<featureRow>
		<featureType>gender</featureType>
		<featureItem>masculine</featureItem>
		<featureExplanation>male gender</featureExplanation>
	</featureRow>
	<featureRow>
		<featureType>&nbsp;</featureType>
		<featureItem>feminine</featureItem>
		<featureExplanation>female gender</featureExplanation>
	</featureRow>
	<featureRow>
		<featureType>&nbsp;</featureType>
		<featureItem>neuter</featureItem>
		<featureExplanation>neuter gender</featureExplanation>
	</featureRow>
	<featureRow>
		<featureType>animacy</featureType>
		<featureItem>animate</featureItem>
		<featureExplanation>persons and animals</featureExplanation>
	</featureRow>
	<featureRow>
		<featureType>&nbsp;</featureType>
		<featureItem>-animate</featureItem>
		<featureExplanation>inanimate objects</featureExplanation>
	</featureRow>
	<featureRow>
		<featureType>class</featureType>
		<featureItem>class_##</featureItem>
		<featureExplanation>(replace ## with the class number up to 99)</featureExplanation>
	</featureRow>
</featureChart>

</instruction>
	<form section="pron" transparent="yes">
		<buttons>
			<button id="PronAgrBack" value="Back" goto="PronCase.htm"/>
			<button id="PronAgrNext" value="Next" goto="PronRefl.htm"/>
		</buttons>
	</form>
</page>
